<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE taglib PUBLIC "-//Sun Microsystems, Inc.//DTD JSP Tag Library 1.2//EN"
        "http://java.sun.com/dtd/web-jsptaglibrary_1_2.dtd">


<taglib>
    <tlib-version>1.0</tlib-version>
    <jsp-version>1.2</jsp-version>
    <short-name>customTags</short-name>
    <description>Tag library for Car Rent application</description>
    <tag>
        <name>date</name>
        <tag-class>com.example.project.utils.tag.CurrentDateTag</tag-class>
        <body-content>empty</body-content>
    </tag>
    <tag>
        <name>minAge</name>
        <tag-class>com.example.project.utils.tag.AdultAgeTag</tag-class>
        <body-content>empty</body-content>
    </tag>
</taglib>